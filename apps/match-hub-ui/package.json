{
  "name": "match-hub-ui",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run && npm run start:e2e",
    "start:e2e": "ng serve --port 4300",
    "test": "ng test --no-watch --browsers=ChromeHeadless --code-coverage",
    "test:unit": "ng test --no-watch --browsers=ChromeHeadless",
    "test:ui": "ng test",
    "test:e2e": "npm run build && npm run serve-and-test",
    "test:full": "npm run test && echo 'Success'",
    "test:full-bak": "npm run test && npm run test:e2e && echo 'Success'",
    "test:push": "npm run test && npm run test:unit && npm run test:e2e;echo 'Success'",
    "serve-and-test": "start-server-and-test start:e2e http://localhost:4300 cypress:run",
    "coby": "jest --coverage --coverageReporters=cobertura --coverageDirectory=coverage",
    "report:clean": "rm -rf cypress/results && rm -rf mochawesome-report",
    "report:generate": "npm run report:clean && mkdir -p mochawesome-report && cypress run --env mochawesome=true && npm run report:merge",
    "report:html": "marge mochawesome-report/mochawesome-bundle.json -o mochawesome-report --inline",
    "report:merge": "mochawesome-merge cypress/results/*.json > mochawesome-report/mochawesome-bundle.json"
  },
  "private": true,
  "dependencies": {
    "@angular/cdk": "^20.2.10",
    "@angular/common": "^20.0.0",
    "@angular/compiler": "^20.0.0",
    "@angular/core": "^20.0.0",
    "@angular/forms": "^20.0.0",
    "@angular/material": "^20.2.10",
    "@angular/platform-browser": "^20.0.0",
    "@angular/router": "^20.0.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "uuid": "^13.0.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^20.3.7",
    "@angular/build": "^20.0.1",
    "@angular/cli": "^20.0.1",
    "@angular/compiler-cli": "^20.0.0",
    "@testing-library/angular": "^18.1.0",
    "@testing-library/dom": "^10.4.1",
    "@types/jasmine": "~5.1.0",
    "@types/uuid": "^10.0.0",
    "cypress": "^15.5.0",
    "jasmine-core": "~5.7.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "karma-junit-reporter": "latest",
    "mocha-junit-reporter": "^2.2.1",
    "mochawesome": "^7.1.4",
    "mochawesome-merge": "^5.0.0",
    "start-server-and-test": "^2.0.3",
    "typescript": "~5.8.2"
  }
}
